<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="AnchorPane" blendMode="MULTIPLY" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="323.0" prefWidth="325.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <ImageView fitHeight="325.0" fitWidth="400" layoutX="0" layoutY="0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../image/EMRED.png" />

            </image>
        </ImageView>
        <Button fx:id="play" layoutX="14.0" layoutY="286.0" mnemonicParsing="false" onAction="#click" prefHeight="25.0" prefWidth="82.0" text="Play">
            <font>
                <Font name="System Bold Italic" size="15.0" />
            </font>
        </Button>
        <Button fx:id="exit" layoutX="107.0" layoutY="286.0" mnemonicParsing="false" onAction="#click" prefHeight="25.0" prefWidth="82.0" text="Exit">
            <font>
                <Font name="System Bold Italic" size="15.0" />
            </font>
        </Button>
      <MenuButton layoutX="226.0" layoutY="7.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="93.0" text="Settings">
        <items>
          <RadioMenuItem mnemonicParsing="false" text="Music" onAction="#startMusic"  fx:id="music"/>
          <RadioMenuItem mnemonicParsing="false" text="Sound"  onAction="#startSound" fx:id="sound"/>
        </items>
         <font>
            <Font name="System Bold Italic" size="13.0" />
         </font>
      </MenuButton>
    </children>
</AnchorPane>
